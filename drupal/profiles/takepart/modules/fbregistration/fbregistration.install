<?php
/**
 * Implementation of hook_install().
 */
function fbregistration_install() {
  drupal_set_message(st('The Facebook registration module has been installed.'));
}

/**
 * Implementation of hook_uninstall().
 */
function fbregistration_uninstall() {
  drupal_set_message(st('The Facebook registration module has been uninstalled.'));
}
