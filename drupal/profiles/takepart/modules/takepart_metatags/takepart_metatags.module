/**
 * Implements hook_init().
 */
function takepart_metatags_init() {

   $extra_meta_tags = array(
      'facebook_page' => array(
            '#type' => 'html_tag',
            '#tag' => 'meta',
            '#attributes' => array(
                'property' => 'fb:page_id',
                'content' =>  '9287842439'
            ),
       ),
       'facebook_app' => array(
           '#type' => 'html_tag',
           '#tag' => 'meta',
           '#attributes' => array(
               'property' => 'fb:app_id',
               'content' =>  '247137505296280'
           ),
       ),
       'facebook_admins' => array(
           '#type' => 'html_tag',
           '#tag' => 'meta',
           '#attributes' => array(
               'property' => 'fb:admins',
               'content' =>  '789650214,1327833247'
           ),
       ),
   );

   foreach($extra_meta_tags as $key=>$metatag) {
     drupal_add_html_head( $metatag, $key);
   } 

}