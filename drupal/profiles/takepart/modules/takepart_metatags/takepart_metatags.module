           '#type' => 'html_tag',
           '#tag' => 'meta',
           '#attributes' => array(
               'property' => 'fb:app_id',
               'content' =>  '247137505296280'
           ),
       ),
       'facebook_admins' => array(
           '#type' => 'html_tag',
           '#tag' => 'meta',
           '#attributes' => array(
               'property' => 'fb:admins',
               'content' =>  '789650214,1327833247,792568495,658568746,13304443,100003533144273,721281337'
           ),
       ),
   );

   foreach($extra_meta_tags as $key=>$metatag) {
     drupal_add_html_head( $metatag, $key);
   } 

}